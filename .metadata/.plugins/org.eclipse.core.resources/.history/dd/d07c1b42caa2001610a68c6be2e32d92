package utd.nikita.code;

public class LinkedList {

	public int length(ListNode l) {
		int length = 0;
		while (l != null) {
			length++;
			l = l.next;
		}
		return length;
	}

	public ListNode removeNthFromEnd(ListNode head, int n) {
		int length = length(head);
		if(length <n)
			return null;
		ListNode New = head;
		else{
			int removeNode = length - n;
			int i = 0;
			while(i == removeNode){
				i++;
				head = head.next;
			}
			
		}
		return null;

	}
}