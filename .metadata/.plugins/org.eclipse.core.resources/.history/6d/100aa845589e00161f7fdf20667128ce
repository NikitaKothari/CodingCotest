package utd.nikita.code;

public class Solution {

	public String solution(String S, int K) {
		if (S == "" || S.length() == 0)
			return S;
		if (S.length() == 1)
			return S.toUpperCase();

		String resultedString = "";
		int noOfDashes = 0;

		for (int i = 0; i < S.length(); i++) {
			if (S.charAt(i) == '-')
				noOfDashes++;
		}
		if (K <= noOfDashes) {

		}

		return S;

	}

}
